generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// For migration you have to specify a file of shcema and run this command
// prisma migrate dev --schema=./schema.prisma init
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
